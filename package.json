{
  "name": "zone-clone",
  "version": "1.0.0",
  "description": "Zoom Clone using WebRTC and WebSocket",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "tsc client/src/app.ts --outDir client/public -w",
    "dev:css": "npx tailwindcss --postcss -i client/src/css/tailwind.css -o client/public/style.css -w",
    "dev": "concurrently \"yarn run dev:server\" \"yarn run dev:client\" \"yarn run dev:css\"",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.13",
    "@types/ws": "^7.4.7",
    "autoprefixer": "^10.3.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.7",
    "ts-node": "^10.1.0",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.2.0",
    "ws": "^8.0.0"
  }
}
